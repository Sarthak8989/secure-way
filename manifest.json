{
    "manifest_version":2,
    "name": "SecureWay",
    "description":"Secure your browsing experience",
    "version":"1.0",
    "icons":{
        "16":"/images/firefox-icon-16.png",
        "48":"/images/firefox-icon-48.png",
        "128":"/images/firefox-icon-128.png"
    },
    "page_action":{
        "default_icon":{
            "16":"/images/firefox-icon-16.png",
            "48":"/images/firefox-icon-48.png",
            "128":"/images/firefox-icon-128.png"
        }
    },
    "content_scripts":[{
        "matches":["https://www.google.com/*"],
        "css":["main.css"]
    }]
}